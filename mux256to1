module mux256to1( 
    input [1023:0] in,
    input [7:0] sel,
    output [3:0] out );
    
    wire [3:0] arr[255:0];
    genvar i;
    generate
        for(i=0; i<256; i=i+1) begin : pack
            assign arr[i] = in[4*i+3 : 4*i];
        end
    endgenerate
    assign out = arr[sel];
    
endmodule
